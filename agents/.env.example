# Agent A (Coinbase CDP Agent)
# Generate Solana keypair: node -e "const {Keypair} = require('@solana/web3.js'); const bs58 = require('bs58'); const kp = Keypair.generate(); console.log('Private:', bs58.encode(kp.secretKey)); console.log('Public:', kp.publicKey.toBase58());"
AGENT_A_PRIVATE_KEY=your-base58-encoded-private-key-here
AGENT_A_NAME=CoinbaseCDP

# Agent B (Crossmint Agent)
AGENT_B_PRIVATE_KEY=your-base58-encoded-private-key-here
AGENT_B_NAME=Crossmint

# Google Gemini API Key (for AI reasoning)
# Get from: https://makersuite.google.com/app/apikey
BID_GEMINI_API_KEY=your-gemini-api-key

# Server Configuration
BID_SERVER_URL=http://localhost:3000

# Optional: Tunnel URL for exposing localhost to Firecrawl
# Use Cloudflare Tunnel or ngrok to expose localhost
# TUNNEL_URL=https://your-tunnel-url.com

# Optional: Ad Spot Configuration
# AD_SPOT_ID=prime-banner
